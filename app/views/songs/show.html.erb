<h1>Song Releases</h1>
<%= form_for(@songs) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>

  <%= f.label :genre %>
  <%= f.text_field :genre %>

  <%= f.label :release_year %>
  <%= f.datefield :release_year %>

  <%= f.hidden :released %>

  <%= f.submit %>
<%= end %>



<h2><%= @song.title %></h2>
<%= @song.artist_name %>
<%= form_tag song_path(@song.id), method: "delete" do %>
  <%= submit_tag "Delete #{@song.title}" %>
<% end %>